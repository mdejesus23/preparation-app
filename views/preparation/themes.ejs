<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/user.css" />
  </head>

  <body>
  <%- include('../includes/navigation.ejs') %>
    
  <main>
    <% if (themes.length > 0) { %>
        <div>
            <% themes.forEach((theme) => { %>
                <ul>
                    <li><%= theme.themeTitle %></li>
                </ul>
            <% }) %>
        </div>
    <% } else { %>    
    <h1>No Themes Found!</h1>
    <% } %>
</main>
    <%- include('../includes/end.ejs') %>

